<?xml version="1.0" encoding="UTF-8" ?> 

<taglib xmlns="http://java.sun.com/xml/ns/j2ee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
                        web-jsptaglibrary_2_0.xsd" 
    version="2.0"> 
     
    <description>EL에서 함수실행</description> 
    <tlib-version>1.0</tlib-version> 
    <short-name>ELfunctions</short-name> 
    <uri>/ELFunctions</uri> 
     
    <function> 
        <description>댓글갯수</description> 
        <name>frcount</name>
        <function-class>                  
            spring.utility.gobook.Utility
        </function-class> 
        <function-signature> 
            int rcount( int, spring.model.freeboard_reply.Freeboard_replyDAO ) 
        </function-signature> 
    </function> 
    <function> 
        <description>직업명생성</description> 
        <name>jobvalue</name>
        <function-class>                  
            spring.utility.gobook.Utility
        </function-class> 
        <function-signature> 
            java.lang.String getCodeValue( java.lang.String ) 
        </function-signature> 
    </function> 
    <function> 
        <description>new이미지 생성</description> 
        <name>newimg</name>
        <function-class>                  
            spring.utility.gobook.Utility
        </function-class> 
        <function-signature> 
            boolean compareDay( java.lang.String ) 
        </function-signature> 
    </function> 
    
</taglib> 